<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="owl_playground.card" owl="1">
        <div class="card">
            <div class="card-body">
                <h5 class="card-title" t-if="props.slots.title">
                    <t t-slot="title"/>
                </h5>
                <t t-slot="default"/>
            </div>
        </div>
    </t>
</templates>
